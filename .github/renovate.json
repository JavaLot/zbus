{
  "packageRules": [
    {
      "matchManagers": [
        "github-actions"
      ],
      "commitMessagePrefix": "⬆️  "
    },
    {
      "matchManagers": [
        "cargo"
      ],
      "matchUpdateTypes": [
        "major",
        "minor"
      ],
      "commitMessagePrefix": "⬆️  ",
      "commitMessageTopic": "{{depName}}",
      "lockFileMaintenance": {
        "enabled": true
      }
    }
  ]
}